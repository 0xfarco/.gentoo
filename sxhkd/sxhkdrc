#	pactl set-sink-volume @DEFAULT_SINK@ +5%
#	pamixer -i 5

#XF86AudioLowerVolume
#	pactl set-sink-volume @DEFAULT_SINK@ -5%
#	pamixer -d 5

#XF86AudioMute
#	pactl set-sink-mute @DEFAULT_SINK@ toggle 
#	pamixer --toggle-mute

XF86Audio{Raise,Lower}Volume  
  pulsemixer --change-volume {+,-}1  
 
XF86AudioMute  
  pulsemixer  --toggle-mute

