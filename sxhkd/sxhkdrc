XF86MonBrightnessDown
  xbacklight -dec 5

XF86MonBrightnessUp
  xbacklight -inc 5

XF86Audio{Raise,Lower}Volume  
  pulsemixer --change-volume {+,-}1  
 
XF86AudioMute  
  pulsemixer  --toggle-mute

alt + r
  rofi -show run

alt + shift + r
  rofi -show drun

alt + f
  firefox-bin

alt + shift + f
  firefox-bin --private-window

alt + c
  googe-chrome-stable

alt + shift + c
  google-chrome-stable --incognito

alt + b
  brave

alt + shift + b
  brave --incognito

alt + shift + w
  camtoggle

alt + shift + e
  st -e yazi

alt + p
  sysact

alt + m
  st -e calcurse

Print
  flameshot gui
